package c1.sc1;

import utils.FractionNumber;


/**
 * 一共有n种不同的coupons，每种coupons的概率相同。问期望多少次可以得到n种coupons，以带分数形式输出。
 *  1<=n<=33
 * @author jimmy
 *
 */
public class P_1_1 {
	
	private long couponStyleNumber =20; 
	
	FractionNumber getExpectedNextCouponStepLength(int noNeedStyleNumber){
		if(couponStyleNumber-noNeedStyleNumber==0){
			throw new Exception("The denominator of the fraction cannot be zero!");
		}
		return new FractionNumber(couponStyleNumber,couponStyleNumber-noNeedStyleNumber);
	}
	
	
	public static void main(String[] args){
		
	}
	
	
	
}
